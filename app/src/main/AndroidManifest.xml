<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projet.musichall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfilActivity"/>
        <activity android:name=".Connexion"/>
        <activity android:name=".Inscription"/>
        <activity android:name=".Search.SearchActivity"/>
        <activity android:name=".Search.CriteresMusiciens"/>
        <activity android:name=".Search.CriteresGroupe"/>
        <activity android:name=".group.GroupActivity"/>
        <activity android:name=".Menu"/>
        <activity android:name=".ShouldLogin"/>
        <activity
        android:name="com.projet.musichall.BaseActivity"
        android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden"
        android:uiOptions="splitActionBarWhenNarrow"
        android:label="WeTrip" />
    </application>

</manifest>